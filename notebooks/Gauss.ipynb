   "source": [
    "# Funcion que diagonaliza y resuelve un sistema a partir de la matriz asociada\n",
    "\n",
    "from sympy import *\n",
    "init_session()\n",
    "\n",
    "def mi_gauss(M) :\n",
    "    return(M, M.LUdecomposition()[1],M[:,:-1].LUsolve(M.col(-1)))\n",
    "\n",
    "# Ejemplo\n",
    "\n",
    "M=Matrix([[1,1,-1,5],[2,1,1,-4],[-1,3,-2,6]])\n",
    "mi_gauss(M)"
   ]
  }
